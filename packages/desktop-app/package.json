{
  "name": "Salad",
  "version": "0.4.0",
  "description": "Salad Technologies Desktop Application",
  "author": "Salad Technologies <dev@salad.io>",
  "homepage": "https://www.salad.io/",
  "license": "MIT",
  "main": "dist/main/main.js",
  "files": [
    "lib"
  ],
  "engineStrict": true,
  "engines": {
    "node": "12.8.1",
    "yarn": ">=1.10.0"
  },
  "scripts": {
    "start": "electron-webpack dev",
    "build": "electron-webpack",
    "dist": "yarn build && electron-builder",
    "release": "yarn build && electron-builder --publish always"
  },
  "electronWebpack": {
    "title": true,
    "staticSourceDirectory": "static",
    "main": {
      "extraEntries": [
        "@/preload.js"
      ],
      "sourceDirectory": "src"
    },
    "renderer": {
      "sourceDirectory": null
    }
  },
  "devDependencies": {
    "@types/node": "12.12.24",
    "@types/node-notifier": "5.4.0",
    "electron": "7.1.9",
    "electron-builder": "22.2.0",
    "electron-reload": "1.5.0",
    "electron-webpack": "2.7.4",
    "electron-webpack-ts": "3.2.0",
    "ts-loader": "6.2.1",
    "tslint": "5.20.1",
    "tslint-config-standard": "9.0.0",
    "tslint-loader": "3.5.4",
    "typescript": "3.7.4",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10"
  },
  "dependencies": {
    "@sentry/electron": "1.2.0",
    "auto-launch": "5.0.5",
    "axios": "0.19.1",
    "decompress-zip": "0.2.2",
    "electron-log": "4.0.3",
    "electron-updater": "4.2.0",
    "is-online": "8.2.1",
    "node-notifier": "6.0.0",
    "source-map-support": "0.5.16",
    "systeminformation": "4.19.1"
  }
}
